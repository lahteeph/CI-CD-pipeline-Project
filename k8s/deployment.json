{
  "kind": "Deployment", 
  "spec": {
    "replicas": 1, 
    "template": {
      "spec": {
        "containers": [
          {
            "image": "kodecamp-app:latest", 
            "name": "kodecamp-app", 
            "ports": [
              {
                "containerPort": 8080
              }
            ]
          }
        ]
      }, 
      "metadata": {
        "labels": {
          "app": "kodecamp-app"
        }
      }
    }, 
    "selector": {
      "matchLabels": {
        "app": "kodecamp-app"
      }
    }
  }, 
  "apiVersion": "apps/v1", 
  "metadata": {
    "labels": {
      "app": "kodecamp-app"
    }, 
    "name": "kodecamp-app"
  }
}
